<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.jeremy.bandaide"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".song"
            android:label="@string/title_activity_song__activity_"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".song_add">
        </activity>
        <activity android:name=".people">
        </activity>
        <activity android:name=".people_add">
        </activity>
        <activity android:name=".people_view">
        </activity>
        <activity android:name=".venue">
        </activity>
        <activity android:name=".venue_add">
        </activity>
        <activity android:name=".venue_view">
        </activity>
        <activity android:name=".gigs">
        </activity>
        <activity android:name=".gigs_add">
        </activity>
        <activity android:name=".gigs_view">
        </activity>
        <activity android:name=".setlists">
        </activity>
        <activity
            android:name=".song_view"
            android:label="@string/title_activity_song_view"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".song_edit">
        </activity>
        <activity android:name=".venue_edit">
        </activity>
        <activity android:name=".people_edit">
        </activity>
        <activity android:name=".gigs_edit">
        </activity>
        <activity android:name=".setlists_add">
        </activity>
        <activity android:name=".setlists_view">
        </activity>
        <activity android:name=".setlists_edit_basic">
        </activity>
        <activity android:name=".setlists_edit_songs">
        </activity>
        <activity android:name=".setlists_view_songs">
        </activity>
    </application>

</manifest>
